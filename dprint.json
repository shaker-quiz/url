{
  "typescript": {
    "lineWidth": 180,
    "quoteStyle": "preferSingle",
    "semiColons": "asi",
    "importDeclaration.forceSingleLine": true,
    "arrowFunction.useParentheses": "preferNone",
    "useBraces": "maintain",
  },
  "json": {},
  "includes": [
    "**/*.js"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.9.wasm",
    "https://plugins.dprint.dev/json-0.20.0.wasm",
  ]
}